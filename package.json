{
  "name": "jellyskin",
  "version": "12.0.0",
  "description": "A vibrant skin for jellyfin made with SASS",
  "files":["/dist"],
  "scripts": {
    "watch:sass": "sass  --no-source-map --watch src/:dev/",
    "serve": "browser-sync start --server dev --files dev",
    "build": "sass src/:dist/",
    "postbuild": "postcss dist/*.css -u autoprefixer cssnano -r",
    "postbuild:dev": "postcss dev/*.css -u autoprefixer cssnano -r",
    "start": "npm-run-all --parallel watch:sass postbuild:dev serve"
  },
  "np": {
		"yarn": false,
		"contents": "dist",
    "anyBranch": true
	},
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.17",
    "np": "^7.6.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prayag17/JellySkin.git"
  },
  "keywords": [
    "jellyfin",
    "css",
    "jellyskin"
  ],
  "author": "prayag17",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/prayag17/JellySkin/issues"
  },
  "homepage": "https://github.com/prayag17/JellySkin#readme"
}
